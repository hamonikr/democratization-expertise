{
  "name": "askos-user",
  "version": "1.1.2",
  "productName": "askos-user",
  "description": "askos 사용자 앱",
  "homepage": "https://hamonikr.org/",
  "author": "HamoniKR Team <hamonikr-packages@googlegroups.com>",
  "keywords": [
    "askos",
    "hamonikr",
    "hamonikros"
  ],
  "main": "main.js",
  "scripts": {
    "cache-clean": "rm -rf ~/.electron",
    "start": "electron main.js --no-sandbox",
    "test": "electron ./testJquery/test.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "build:linux64": "electron-builder --linux --x64",
    "build:win64": "electron-builder --win  --x64"
  },
  "files": [
    "*.js",
    "license.txt",
    "readme.txt",
    "app",
    "bin",
    "build"
  ],
  "dependencies": {
    "archiver": "3.0.0",
    "auto-launch": "5.0.5",
    "axios": "^0.21.1",
    "charset": "^1.0.1",
    "cookie-parser": "^1.4.5",
    "csurf": "^1.11.0",
    "dialog": "^0.3.1",
    "dialogs": "^2.0.1",
    "electron-native-notification": "^1.2.1",
    "electron-store": "2.0.0",
    "electron-window-state": "5.0.0",
    "fs-extra": "7.0.0",
    "iconv-lite": "^0.5.0",
    "ip": "^1.1.5",
    "jquery": "^3.4.1",
    "koa": "^2.10.0",
    "koa-router": "^7.4.0",
    "markdown-it": "8.4.2",
    "nedb": "1.8.0",
    "node-id-generator": "^0.1.1",
    "node-notifier": "^6.0.0",
    "open": "^7.0.0",
    "request": "^2.88.0",
    "shortid": "^2.2.15",
    "uniqid": "^5.1.0",
    "unirest": "^0.6.0",
    "unzipper": "0.9.3",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "electron": "^7.0.2",
    "electron-builder": "^22.3.2",
    "pkg": "^4.4.0"
  },
  "build": {
    "icon": "./build/icons",
    "productName": "askos-user",
    "appId": "askos.user.application",
    "asar": true,
    "protocols": {
      "name": "askos-user",
      "schemes": [
        "askos-user"
      ]
    },
    "win": {
      "target": [
        "zip",
        "nsis"
      ],
      "icon": "res/bin/icon.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "res/bin/iconshm/",
      "desktop": {
        "Encoding": "UTF-8"
      }
    },
    "deb": {
      "afterInstall": "res/linux/after-install.tpl",
      "afterRemove": "res/linux/after-remove.tpl"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "directories": {
      "buildResources": "resources/installer/",
      "output": "dist/",
      "app": "."
    }
  }
}
