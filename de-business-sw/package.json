{
  "name": "askos-support",
  "version": "1.0.6",
  "description": "askos support Manager for HamoniKR",
  "homepage": "http://askos.co.kr/",
  "author": "HamoniKR Team <hamonikr-packages@googlegroups.com>",
  "license": "Apache-2.0",
  "readme": "readme.txt",
  "main": "main.js",
  "scripts": {
    "cache-clean": "rm -rf ~/.electron",
    "start": "electron main.js --no-sandbox",
    "test": "electron ./testJquery/test.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "build:linux64": "electron-builder --linux --x64",
    "build:win64": "electron-builder --win  --x64"
  },
  "files": [
    "*.js",
    "license.txt",
    "readme.txt",
    "public",
    "bin"
  ],
  "dependencies": {
    "acron": "^1.0.5",
    "app-root-path": "^3.0.0",
    "chokidar": "^3.3.1",
    "compass": "^0.1.1",
    "create-file": "^1.0.1",
    "date-utils": "^1.2.21",
    "dialogs": "^2.0.1",
    "easytimer.js": "^4.1.1",
    "electron-installer-squirrel-windows": "^1.3.0",
    "electron-ipc-promise": "^0.1.7",
    "electron-log": "^3.0.5",
    "electron-modal": "^1.0.0",
    "electron-native-notification": "^1.2.1",
    "electron-notification": "^1.0.0",
    "electron-notify": "^0.1.0",
    "electron-packager": "^14.2.0",
    "electron-sudo": "^4.0.12",
    "electron-window-manager": "^1.0.6",
    "electron-window-state": "^5.0.3",
    "electron-windows-notifications": "^3.0.6",
    "es6-promise": "^4.2.8",
    "es6-promise-polyfill": "^1.2.0",
    "es6-promise-pool": "^2.5.0",
    "express": "^4.17.1",
    "express-ws": "^2.0.0",
    "findit": "^2.0.0",
    "fs": "0.0.1-security",
    "ip": "^1.1.5",
    "jquery": "^3.4.1",
    "koa": "^2.11.0",
    "koa-handlebars": "^1.0.0",
    "koa-router": "^8.0.8",
    "line-reader": "^0.4.0",
    "minimist": "^1.2.0",
    "mkdirp": "^1.0.4",
    "mkdirp-promise": "^5.0.1",
    "moment": "^2.24.0",
    "mysql": "^2.18.1",
    "node-notifier": "^6.0.0",
    "node-watch": "^0.6.3",
    "open": "^7.0.3",
    "os": "^0.1.1",
    "ping": "^0.2.3",
    "ping-wrapper": "0.0.3",
    "react-countdown-circle-timer": "^1.1.0",
    "read-last-lines": "^1.7.2",
    "sudo-prompt": "^9.1.1",
    "time-counter": "^1.0.0",
    "time-stamp": "^2.2.0",
    "tiny-timer": "^1.4.0",
    "uniqid": "^5.2.0",
    "unirest": "^0.6.0",
    "url": "^0.11.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^4.4.2"
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@pms.invesume.com:7999/~hckim/amt-webcmd.git"
  },
  "keywords": [
    "hamonikr"
  ],
  "devDependencies": {
    "electron": "^7.0.2",
    "electron-builder": "^22.3.2",
    "pkg": "^4.4.0"
  },
  "build": {
    "icon": "./build/icons",
    "productName": "askos-support",
    "appId": "hamonikr.askos.support",
    "asar": true,
    "protocols": {
      "name": "askos-support",
      "schemes": [
        "askos-support"
      ]
    },
    "win": {
      "target": [
        "zip",
        "nsis"
      ],
      "icon": "res/bin/icon.ico"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "res/bin/iconshm/",
      "desktop": {
        "Encoding": "UTF-8"
      }
    },
    "deb": {
      "afterInstall": "res/linux/after-install.tpl",
      "afterRemove": "res/linux/after-remove.tpl"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true
    },
    "directories": {
      "buildResources": "resources/installer/",
      "output": "dist/",
      "app": "."
    }
  }
}
